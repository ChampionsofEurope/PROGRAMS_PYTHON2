france = {"PARIS":"LORIENT","MARSIELLE":"NICE","BORDAUEX":"LYON","STRASBOURGE":"TOLOUOSE"}
print(france)

#Length
print(len(france))

#Acessing Items
pari = france.get("PARIS")
print(pari)

lol = france.keys()
print(lol)

rolp = france.get("MARSIELLE")
print(rolp)

pol = france.values()
print(pol)

#NEW ITEMS
print(france)
france.items()

france["PARIS"] = "LILLE"
print(france)
